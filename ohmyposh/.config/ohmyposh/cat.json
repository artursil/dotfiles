{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue":  "#89B4FA",
    "green":  "#286983",
    "gold": "#edc387",
    "rose": "#ebbcba",
    "love": "#b4637a",
    "love2": "#eb6f92",
    "subtle": "#797593",
    "text": "#575279",
    "iris": "#9ccfd8",
    "muted": "#6e6a86"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:muted",
          "style": "plain",
          "template": "{{.Icon}}  ",
          "type": "os"
        },
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": " ",
          "type": "session"
        },
        {
          "foreground": "p:rose",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "full"
          },
          "style": "plain",
          "template": "îªƒ  {{ .Path }} ",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:love",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:green",
          "style": "plain",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}   ",
          "type": "python"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}

